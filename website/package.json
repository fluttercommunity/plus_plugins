{
  "name": "website",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "check:linting": "eslint . --max-warnings=0",
    "check:formatting": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "check:typescript": "tsc",
    "check:spelling": "./node_modules/.bin/spellchecker --quiet --files=\"docs/**/*.{md,mdx}\" --dictionaries=\"./dictionary.js\" --reports=\"./spelling.json\" --plugins spell indefinite-article repeated-words syntax-mentions syntax-urls frontmatter"
  },
  "engines": {
    "node": ">=16.13.1",
    "npm": ">=7"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@docusaurus/core": "^2.1.0",
    "@docusaurus/module-type-aliases": "^2.1.0",
    "@docusaurus/plugin-google-gtag": "^2.1.0",
    "@docusaurus/preset-classic": "^2.1.0",
    "@types/lodash.get": "^4.4.6",
    "@types/react": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "axios": "^0.22.0",
    "classnames": "^2.3.1",
    "compare-versions": "^4.1.0",
    "docusaurus-plugin-sass": "^0.2.0",
    "dotenv": "^10.0.0",
    "lodash.get": "^4.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-medium-image-zoom": "^4.3.4",
    "react-router-dom": "^5.2.0",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "prettier": "^2.3.0",
    "spellchecker-cli": "^4.4.0",
    "typescript": "^4.2.4"
  }
}
